typeSystem propDNF =
    types _:var, _:literal, _:clause, _:dnf, _:nonvar, _:nonliteral, _:nonclause
    order var < literal,
          nonvar < literal,
          literal < clause,
          nonliteral < clause,
          clause < dnf,
          nonclause < dnf

conSpec propDNFG:propDNF =
  constructors
    conj : [literal, clause] -> nonliteral,
    disj : [clause, dnf] -> nonclause,
    neg : [var] -> nonvar
