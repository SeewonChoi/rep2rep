import arith

typeSystem contTable =
 imports arith
 types _:name, _:oneDim, _:twoDim, _:table, _:eventVar
 order oneDim < table, twoDim < table, eventVar < name

conSpec contTableG:contTable =
 imports arithG
 constructors
  notName : [name] -> name,
  buildOne : [name,numExp] -> oneDim,
  buildTwo : [oneDim,oneDim,numExp] -> twoDim,
  combine : [table,table] -> table